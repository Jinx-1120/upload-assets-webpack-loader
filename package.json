{
  "name": "upload-assets-webpack-loader",
  "version": "1.0.1",
  "description": "custome upload assets for webpack",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "keywords": [
    "webpack",
    "custome upload",
    "assets"
  ],
  "author": "Jinx-1120",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "webpack": "^5.73.0"
  },
  "dependencies": {
    "loader-utils": "^3.2.1",
    "mime-types": "^2.1.35",
    "mkdirp": "^3.0.1",
    "schema-utils": "^4.2.0"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/loader-utils": "^2.0.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "18.18.12",
    "@types/webpack": "^5.28.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  }
}
